                <for variable="c2" start="1" end="10" direction="inc" step="1">
                    <assign variable="add" expression="c1+c2"/>
                    <output expression="c1&amp;&quot;+&quot;&amp;c2&amp;&quot;=&quot;&amp;add" newline="True"/>
                </for>
            </for>
        </body>
