    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="addition table"/>
            <comment text="c1 is main counter and the c2 is another counter which keeps on changing and gets added to c2"/>
            <declare name="c1, c2, add" type="Integer" array="False" size=""/>
